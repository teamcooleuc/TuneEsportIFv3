@page
@inject SignInManager<TuneEsportIfv2User> signInManager
@inject UserManager<TuneEsportIfv2User> userManager
@using TuneEsportIFv2.Areas.Identity.Data
@using System.Security.Cryptography.X509Certificates
@using Microsoft.AspNetCore.Identity.UI.V4.Pages.Account.Internal
@using Microsoft.CodeAnalysis.Differencing
@model TuneEsportIFv2.Areas.Identity.Pages.Profiles.IndexModel
@{
}


<form id="profile-form" method="post" enctype="multipart/form-data">
    <div class="row">
        <div class="col-md-6">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Username"></label>
                <input asp-for="Username" class="form-control" disabled="disabled" />
                <span asp-validation-for="Username" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Input.Rank"></label>
                <input asp-for="Input.Rank" class="form-control" />
                <span asp-validation-for="Input.Rank" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Input.Team"></label>
                <input asp-for="Input.Team" class="form-control" />
                <span asp-validation-for="Input.Team" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Input.ClubName"></label>
                <input asp-for="Input.ClubName" class="form-control" />
                <span asp-validation-for="Input.ClubName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Input.Nick"></label>
                <input asp-for="Input.Nick" class="form-control" />
                <span asp-validation-for="Input.Nick" class="text-danger"></span>
            </div>
            <button id="update-profile-button" type="submit" class="btn btn-primary">Save</button>
        </div>
    </div>

</form>
