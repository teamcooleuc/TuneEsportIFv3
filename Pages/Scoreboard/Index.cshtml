@page
@model TuneEsportIFv2.Pages.Scoreboard.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TrainingStat.mapsName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TrainingStat.EconomyKnowledge)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TrainingStat.Smokes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TrainingStat.Tactics)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TrainingStat.mapKnowledge)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TrainingStat.Date)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.TrainingStats) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.mapsName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EconomyKnowledge)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Smokes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tactics)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.mapKnowledge)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
        </tr>
}
    </tbody>
</table>
